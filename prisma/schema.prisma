generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Bookmark {
  id String @id @default(cuid())
  url String
  title String
  notes String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  userId String
}